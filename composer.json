{
    "name": "tbessenreither/multi-level-cache",
    "description": "A Symfony bundle providing a high performance multi-level caching system.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Bessenreither"
        }
    ],
    "require": {
        "php": "^8.4",
        "symfony/config": "^7.4",
        "symfony/dependency-injection": "^7.4",
        "symfony/http-kernel": "^7.4",
        "symfony/cache": "^8.0",
        "symfony/http-client": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "symfony/phpunit-bridge": "^7.4",
        "symfony/stopwatch": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Tbessenreither\\MultiLevelCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tbessenreither\\MultiLevelCache\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "8.4.0"
        }
    }
}